
  <div class="row">
    <div class="col-xs-3">
      <h3>Secret Identity: {{model.name}}</h3>
    </div>
    <div class="col-xs-3">
      <h5> First Name:
        {{#if model}}
          {{model.firstName}}
        {{else}}
          N/A
        {{/if}}
      </h5>
      <br>
      <h5> Last Name:
        {{#if model}}
          {{model.lastName}}
        {{else}}
          N/A
        {{/if}}
      </h5>
    </div>
    <div class="col-xs-4 col-xs-offset-1">
      {{#each model.Teams as |team|}}
        <p>{{team.name}}</p>
      {{else}}
        <p> Not apart of a team </p>
      {{/each}}
    </div>
  </div>
